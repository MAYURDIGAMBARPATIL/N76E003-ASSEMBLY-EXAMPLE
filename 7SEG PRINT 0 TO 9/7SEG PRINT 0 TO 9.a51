ORG 00H
	P0M1 EQU 0B1H
	P0M2 EQU 0B2H
	P1M1 EQU 0B3H
	P1M2 EQU 0B4H
		
		MOV P0M1, #00H
		MOV P0M1, #0FFH
		MOV P1M1, #00H
		MOV P1M2, #0FFH
		
START:	MOV A,#00H

AGAIN:  MOV R5,#10 ;COUNTER
		MOV DPTR,#500H
		
HERE:	MOVC A,@A+DPTR
		MOV P1,A
		ACALL DELAY
		INC DPTR
		MOV A,#00H
		DJNZ R5,HERE
		SJMP AGAIN
		
	
		
DELAY:  MOV R0,#5
HERE3:  MOV R1,#5
HERE2: 	MOV R2,#5
HERE1:  DJNZ R2,HERE1
		DJNZ R1,HERE2
		DJNZ R0,HERE3
		RET
ORG 500H
	DB 0C0H,0F9H,0A4H,0B0H,99H,92H,82H,0F8H,80H,90H
		END